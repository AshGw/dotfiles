# rename this to config, if you're using work & personal accounts

Host *
    ForwardAgent yes
    ServerAliveInterval 60
    Compression yes
    PreferredAuthentications publickey
    AddKeysToAgent yes

Host github-kloxme.com
    HostName github.com
    User git
    IdentityFile ~/.ssh/github-kloxme


Host github.com
    HostName github.com
    User git
    IdentityFile ~/.ssh/github
